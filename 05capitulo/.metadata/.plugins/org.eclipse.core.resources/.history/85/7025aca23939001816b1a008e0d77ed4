package garagedoor;

public class GarageDoor {
	
	public void up() {
		System.out.println("Abrir portão");
	}

	public void down() {
		System.out.println("Fecha portão");
	}
	
	public void stop() {
		System.out.println("Parar porta");
	}

	public void lightOn() {
		System.out.println("Ligar a luz da garagem");
	}
	
	public void lightOff() {

	}

}
